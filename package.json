{
  "name": "progressive-scheduler",
  "version": "1.0.0",
  "description": "progressive resource scheduling app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "rethink": "rethinkdb",
    "rethinkdb": "rethinkdb",
    "db:create": "rethink-migrate create",
    "db:up": "rethink-migrate --env ${NODE_ENV:-development} up",
    "db:down": "rethink-migrate --env ${NODE_ENV:-development}down",
    "db:reset": "rethink-migrate --env ${NODE_ENV:-development} down --all"
  },
  "keywords": [
    "progressive",
    "scheduling",
    "offline",
    "react"
  ],
  "author": "Jack Ofnotrade <jooakley@gmail.com> (http://github.com/joaker)",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "connect": "^3.5.0",
    "connect-assetmanager": "0.0.28",
    "connect-assetmanager-handlers": "0.0.18",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "rethink-migrate": "^1.3.1",
    "rethinkdb": "^2.3.3"
  }
}
